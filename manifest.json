{
    "manifest_version": 3,
    "name": "Chrome Tabs Manager",
    "description":"",
    "version": "1.1",
    "icons": {
        "16": "images/logo-16.png",
        "32": "images/logo-32.png",
        "48": "images/logo-48.png",
        "128": "images/logo-128.png"
    },
    "background": {
        "service_worker": "background.js"
        },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "nativeMessaging"
    ]
    
}